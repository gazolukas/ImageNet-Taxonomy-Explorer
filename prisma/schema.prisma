generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Taxonomy {
  id        Int      @id @default(autoincrement())
  path      String   @unique
  name      String
  size      Int
  depth     Int
  createdAt DateTime @default(now())

  @@index([name])
  @@index([depth])
}
